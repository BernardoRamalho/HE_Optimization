#
# 16 values
#

python3 generate_numbers.py 16

# Coef SUM 
for i in {1..100}; do ./no-pre-processed-coef-mean number.txt 97 2 3.2 n16-coef-sum-2; done
for i in {1..100}; do ./no-pre-processed-coef-mean number.txt 97 4 3.2 n16-coef-sum-4; done
for i in {1..100}; do ./no-pre-processed-coef-mean number.txt 97 8 3.2 n16-coef-sum-8; done
for i in {1..100}; do ./no-pre-processed-coef-mean number.txt 97 16 3.2 n16-coef-sum-16; done

# Slot Sum
for i in {1..100}; do ./optimized-rotation-mean number.txt 97 2 3.2 n16-slot-sum-2; done
for i in {1..100}; do ./optimized-rotation-mean number.txt 97 4 3.2 n16-slot-sum-4; done
for i in {1..100}; do ./optimized-rotation-mean number.txt 97 8 3.2 n16-slot-sum-8; done
for i in {1..100}; do ./optimized-rotation-mean number.txt 97 16 3.2 n16-slot-sum-16; done

# Coef Inner
for i in {1..100}; do ./optimized-inner-product number.txt 97 2 3.2 n16-slot-inner-2; done
for i in {1..100}; do ./optimized-inner-product number.txt 97 4 3.2 n16-slot-inner-4; done
for i in {1..100}; do ./optimized-inner-product number.txt 97 8 3.2 n16-slot-inner-8; done
for i in {1..100}; do ./optimized-inner-product number.txt 97 16 3.2 n16-slot-inner-16; done

# Slot Inner
for i in {1..100}; do ./coef-inner-product number.txt 97 2 3.2 n16-coef-inner-2; done
for i in {1..100}; do ./coef-inner-product number.txt 97 4 3.2 n16-coef-inner-4; done
for i in {1..100}; do ./coef-inner-product number.txt 97 8 3.2 n16-coef-inner-8; done
for i in {1..100}; do ./coef-inner-product number.txt 97 16 3.2 n16-coef-inner-16; done

