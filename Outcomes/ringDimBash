#
# 8192 values
#

python3 generate_numbers.py 8192

# Coef SUM 
for i in {1..100}; do ./no-pre-processed-coef-mean number.txt 557057 2048 4.3 n8192-coef-sum-2048; done

# Coef Inner
for i in {1..100}; do ./coef-inner-product number.txt 557057 2048 3.2 n8192-coef-inner-2048; done

